{
  "expo": {
    "name": "Almosdare",
    "slug": "Almosdare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "ios": {
    "infoPlist": {
      "LSApplicationQueriesSchemes": [
        "comgooglemaps",
        "citymapper",
        "uber",
        "lyft",
        "waze"
        ]
      },
      "UIBackgroundModes": [
        "location",
        "fetch"
      ]
    }
  }
